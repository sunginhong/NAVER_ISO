<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.naver.naver_iso.and_dev.Sidemenu00_MainActivity">

    <View
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#1e1e1e" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clipChildren="false">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="211dp"
            android:layout_height="88dp"
            android:layout_marginLeft="-16dp"
            android:layout_marginTop="-16dp"
            app:srcCompat="@drawable/sidemenu_article_sidemenu_shadow" />

        <include layout="@layout/sidemenu00_sidemenu" />

        <include layout="@layout/sidemenu00_sidemenu_icon" />

        <ImageView
            android:id="@+id/sidemenu00_logo"
            android:layout_width="100dp"
            android:layout_height="23dp"
            android:layout_marginLeft="57dp"
            android:layout_marginTop="15dp"
            app:srcCompat="@drawable/sidemenu_article_logo" />
    </RelativeLayout>

</RelativeLayout>
